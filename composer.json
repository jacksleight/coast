{
  "name": "jacksleight/coast",
  "type": "library",
  "description": "Web application framework for PHP",
  "keywords": ["framework", "coast"],
  "homepage": "http://coast.jacksleight.com/",
  "license": "MIT",
  "authors": [
    {"name": "Jack Sleight", "email": "jacksleight@gmail.com"}
  ],
  "require": {
    "php": ">=7.1.0",
    "jakeasmith/http_build_url": "0.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "satooshi/php-coveralls": "^2"
  },
  "autoload": {
    "psr-4": {
      "Coast\\": "library/"
    },
    "files": [
      "functions/array.php",
      "functions/general.php",
      "functions/math.php",
      "functions/string.php"
    ]
  },
  "suggest": {
    "jacksleight/coast-doctrine": "for integration with Doctrine ORM",
    "intervention/image": "required to use Coast\\App\\Image"
  }
}
