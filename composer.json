{
  "name": "jacksleight/coast",
  "type": "library",
  "description": "Web application framework for PHP 5.5+",
  "keywords": ["framework", "coast"],
  "homepage": "http://coastphp.com/",
  "license": "MIT",
  "authors": [
    {"name": "Jack Sleight", "email": "jacksleight@gmail.com"}
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "apigen/apigen": "2.8.*",
    "phpunit/phpunit": "3.7.*",
    "satooshi/php-coveralls": "0.6.*"
  },
  "autoload": {
    "psr-4": {"Coast\\": "lib//Coast//"},
    "files": ["lib/Coast.php"]
